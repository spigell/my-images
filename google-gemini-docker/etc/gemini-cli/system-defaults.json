{
  "general": {
    "disableAutoUpdate": true,
    "vimMode": true,
    "preferredEditor": "vim",
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "theme": "Dracula",
    "useFullWidth": true,
    "hideBanner": true,
    "hideTips": false,
    "showMemoryUsage": true
  },
  "tools": {
    "sandbox": false,
    "autoAccept": true
  },
  "telemetry": {
    "enabled": true,
    "target": "local",
    "outfile": "/home/ubuntu/.gemini/gemini.log"
  },
  "privacy": {
    "usageStatisticsEnabled": false
  }
}
